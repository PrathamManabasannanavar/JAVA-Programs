<%
int marks = Integer.parseInt(request.getParameter("marks"));
String grade = null;
switch(marks/10){
case 9:
case 10:
	grade = "A";
	
case 8:
	grade = "A+";
case 7:
	grade = "B+";
case 6:
	grade = "B";
case 5:
	grade = "C";
case 4:
	grade = "P";
default:
	grade = "F";
}

out.println("grade of the student is " + grade);
%>
</body>
</html>